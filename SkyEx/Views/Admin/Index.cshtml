@* ПАНЕЛЬ АДМИНИСТРАТОРА *@
@model SkyEx.Models.FileModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title> Панель администратора </title>
    <link href="~/css/Styles.css" rel="stylesheet" />
    <link href="~/css/HeaderStyles.css" rel="stylesheet" />
    <link href="~/css/FooterStyles.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap-theme.css" rel="stylesheet" />
</head>
<body>
    <h1> Панель администратора </h1>
    <form asp-action="UploadImage" method="post" enctype="multipart/form-data" asp-controller="FileUpload">
        <div class="form-group">
            <label> Заголовок:  </label> <input type="text" name="user_title" id="user_title" /> <br />
            <label> Изображение:  </label> <input type="file" name="user_image" id="user_image" /> <br />
            <label> Полное наименование:  </label> <input type="text" name="user_full_name" id="full_name" /> <br />
            <label> Ссылка на сайт:  </label> <input type="text" name="user_link_site" id="link_site" /> <br />
        </div>
        <div class="form-group">

        </div>
        <div class="form-group">
            <input type="submit" value="Upload" class="btn btn-default" />
        </div>
    </form>
</body>
</html>
